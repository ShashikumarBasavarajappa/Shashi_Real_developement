<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>Training Example</display-name>
  <welcome-file-list>
    <welcome-file>home.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Test_Example</servlet-name>
    <servlet-class>com.dev.Test_Example</servlet-class>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Test_Example</servlet-name>
    <url-pattern>/test_example</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Send_Redirect</servlet-name>
    <servlet-class>com.dev.Send_Redirect</servlet-class>
    <init-param>
      <param-name>Name</param-name>
      <param-value>Shashikumar J B</param-value>
    </init-param>
  </servlet>
  <context-param>
    <param-name>username</param-name>
    <param-value>Shashi</param-value>
  </context-param>
  <context-param>
    <param-name>username2</param-name>
    <param-value>system</param-value>
  </context-param>
  <context-param>
    <param-name>password</param-name>
    <param-value>oracle</param-value>
  </context-param>
  <servlet-mapping>
    <servlet-name>Send_Redirect</servlet-name>
    <url-pattern>/send_redirect</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Servlet2</servlet-name>
    <servlet-class>com.dev.Servlet2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Servlet2</servlet-name>
    <url-pattern>/Servlet2</url-pattern>
  </servlet-mapping>
  <filter>
    <filter-name>f1</filter-name>
    <filter-class>com.dev.MyFilter</filter-class>
    <init-param>
      <param-name>construction</param-name>
      <param-value>no</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>f1</filter-name>
    <url-pattern>/servlet12</url-pattern>
  </filter-mapping>
</web-app>